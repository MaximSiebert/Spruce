/*
Theme Specific
*/


html {
  box-sizing: border-box;
}

*, *:before, *:after {
  box-sizing: inherit;
}

body {
  background-color: {{ theme.background_color }};
  {% css_block theme.text_typography %}
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

main, .page-title, .footer {
  transition: all 0.6s cubic-bezier(0.19, 1, 0.22, 1);
  opacity: 1;
}

.is-changing {
  main, .page-title, .footer {
    opacity: 0;
  }
}

h1, h2, h3, h4, h5, h6 {
  text-rendering: optimizeLegibility;
  color: {{ theme.headline_typography.color }};
  font-weight: {{ theme.headline_typography.font-weight }};
  font-family: {{ theme.headline_typography.font-family }};
}

h2 {
  font-size: inherit;
  margin-top: 0;
  margin-bottom: 0;
}

p {
  margin-top: 0;
  margin-bottom: 0;
}

img {
  width: 100%;
  display: block; 
  height: auto;
}

a {
  transition: color .15s ease-in;
}

.big-text {
  {% css_block theme.big_text_typography %}
  {% if theme.big_text_typography_size == 'Small' %}
    font-size: 16px;
  {% elsif theme.big_text_typography_size == 'Medium' %}
    font-size: 18px;
    {% elsif theme.big_text_typography_size == 'Large' %}
    font-size: 22px;
  {% endif %};
  @media #{$breakpoint-medium}{
    {% if theme.big_text_typography_size == 'Small' %}
      font-size: 24px;
    {% elsif theme.big_text_typography_size == 'Medium' %}
      font-size: 30px;
      {% elsif theme.big_text_typography_size == 'Large' %}
      font-size: 36px;
    {% endif %};
  }
  @media #{$breakpoint-large}{
    {% if theme.big_text_typography_size == 'Small' %}
      font-size: 28px;
      font-size: 2vw;
    {% elsif theme.big_text_typography_size == 'Medium' %}
      font-size: 40px;
      font-size: 2.8vw;
      {% elsif theme.big_text_typography_size == 'Large' %}
      font-size: 48px;
      font-size: 3.2vw;
    {% endif %};
  }
}

.pagination .big-text, .big-text h2 {
  {% css_block theme.link_typography %}
  {% if theme.big_text_typography_size == 'Small' %}
    font-size: 20px;
  {% elsif theme.big_text_typography_size == 'Medium' %}
    font-size: 24px;
    {% elsif theme.big_text_typography_size == 'Large' %}
    font-size: 32px;
  {% endif %};
  @media #{$breakpoint-medium}{
    {% if theme.big_text_typography_size == 'Small' %}
      font-size: 30px;
    {% elsif theme.big_text_typography_size == 'Medium' %}
      font-size: 36px;
      {% elsif theme.big_text_typography_size == 'Large' %}
      font-size: 42px;
    {% endif %};
  }
  @media #{$breakpoint-large}{
    {% if theme.big_text_typography_size == 'Small' %}
      font-size: 36px;
      font-size: 2.6vw;
    {% elsif theme.big_text_typography_size == 'Medium' %}
      font-size: 48px;
      font-size: 3.4vw;
    {% elsif theme.big_text_typography_size == 'Large' %}
      font-size: 56px;
      font-size: 3.8vw;
    {% endif %};
  }
}

.big-text h2 {
  {% css_block theme.big_text_typography %}
  text-decoration: none;
  margin-bottom: 1rem;
}

.pagination {
  background-color: {{ theme.background_color }}; 
  {% if theme.big_text_typography_size == 'Small' %}
    font-size: 10px;
  {% elsif theme.big_text_typography_size == 'Medium' %}
    font-size: 14px;
  {% elsif theme.big_text_typography_size == 'Large' %}
    font-size: 16px;
  {% endif %};
  @media #{$breakpoint-large}{
    {% if theme.big_text_typography_size == 'Small' %}
      font-size: 14px;
    {% elsif theme.big_text_typography_size == 'Medium' %}
      font-size: 18px;
    {% elsif theme.big_text_typography_size == 'Large' %}
      font-size: 22px;
    {% endif %};
  }
}

/*
------------------------------------------------------------------
Header / Menu Styles
------------------------------------------------------------------
*/

body {
  padding-top: 63px; // Navigation height
}

.header, .menu {
  {% if theme.menu_border %}
    border-bottom: 2px solid;
  {% endif %}
  border-color: {{ theme.menu_border_color }};
}

.header {
  z-index: 10;
  transition: all 0.6s cubic-bezier(0.19, 1, 0.22, 1);
  &.nav-up {
    transform: translateY(-100%);
  }
  &.nav-down {
    transform: translateY(0);
  }
}

.active .header.nav-up {
  transform: translateY(0);
}

.bg-none {
  background: none;
}

.left-auto {
  margin-left: auto;
}

.logo {
  margin: 0;
  font-weight: inherit;
  a {
     {% css_block theme.logo_typography %};
    &:hover {
       {% css_block theme.logo_typography.hover %};
    }
  }
  img {
    max-width: 250px;
    {{ "left" | vendorize_css_declaration: "transform-origin" }}
    {{ 0.2 | slider_val_to_scale_val: theme.logo_size | wrap: "scale(", ")" | vendorize_css_declaration: "transform" }}
  }
}


.menu {
  background-color: {{ theme.menu_background_color }};
  transform: translateY(-100%);
  opacity: 0;
  transition: all 0.6s cubic-bezier(0.19, 1, 0.22, 1);
}

.page-title {
  {% css_block theme.menu_title_typography %};
}

.menu li {
  {% css_block theme.menu_typography %};
  a {
    {% css_block theme.menu_typography %};
    text-decoration-skip: ink;
    &:hover {
      {% css_block theme.menu_typography.hover %};
    }
  }
  ul li a {
    font-size: {{ theme.menu_typography.font-size | divided_by: 1.5 | append:'px' }};
  }
}

.mobile-menu-toggle button {
  {% css_block theme.menu_text_typography %};
  text-decoration-skip: ink;
}

.mobile-menu-toggle:hover button {
  {% css_block theme.menu_text_typography.hover %};
}

.mobile-menu-toggle:hover .hamburger .line {
  background-color: {{ theme.menu_text_typography.hover.color }}
}

.menu-icon:hover {
  text-decoration: none;
}

.nav-container {
  transition: background-color .15s ease-in;
  background-color: {{ theme.menu_background_color }};
}

.active {
  .menu {
    opacity: 1;
    transform: translateY(0);
  }
}

.hamburger .line {
  width: 20px;
  height: 3px;
  background-color: {{ theme.menu_text_typography.color }};
  display: block;
  margin: 5px auto;
  -webkit-transition: all 0.3s ease-in-out;
  -o-transition: all 0.3s ease-in-out;
  transition: all 0.3s ease-in-out;
}

.hamburger:hover {
  cursor: pointer;
}

.active .hamburger .line:nth-child(2){
  opacity: 0;
}

.active .hamburger .line:nth-child(1){
  -webkit-transform: translateY(8px) rotate(45deg);
  -ms-transform: translateY(8px) rotate(45deg);
  -o-transform: translateY(8px) rotate(45deg);
  transform: translateY(8px) rotate(45deg);
}

.active .hamburger .line:nth-child(3){
  -webkit-transform: translateY(-8px) rotate(-45deg);
  -ms-transform: translateY(-8px) rotate(-45deg);
  -o-transform: translateY(-8px) rotate(-45deg);
  transform: translateY(-8px) rotate(-45deg);
}

.dropdown {
  display: none;
  &.open {
    display: block;
  }
}

/*
------------------------------------------------------------------
Footer Styles
------------------------------------------------------------------
*/

.footer {
  background-color: {{ theme.footer_background_color }};
  {% if theme.footer_border %}
    border-top: 2px solid;
  {% endif %}
  border-color: {{ theme.footer_border_color }};
  {% css_block theme.footer_typography %};
  a {
    &.icon {
      text-decoration: none !important;
    }
    {% css_block theme.footer_link_typography %};
    &:hover {
      {% css_block theme.footer_link_typography.hover %};
    }
  }
}

/*
------------------------------------------------------------------
Gallery loop
------------------------------------------------------------------
*/

body.gallery {
  .asset-container {
    background-color: {{ theme.gallery_frames_color_code }}
  }
  .img-container a {
    cursor: zoom-in;
  }
  .asset {
    .caption {
      {% css_block theme.gallery_caption_typography %};
      padding-left: 1rem;
      padding-right: 1rem;
      {% if theme.gallery_frame_size != 'None' %}
        padding-left: 0;
        padding-right: 0;
      {% endif %}
      a {
        {% css_block theme.gallery_caption_link_typography %};
        &:hover {
          {% css_block theme.gallery_caption_link_typography.hover %};
        }
      }
    }
    &.image {
      position: relative;
      line-height: 0;
      img {
        opacity: 0;
        transition: all 0.6s cubic-bezier(0.19, 1, 0.22, 1);
      }
      .lazyloaded {
        opacity: 1;
      }
    }
    &.video {
      background-color: {{ theme.gallery_video_frames_color }};
      .video-overlay:before {
        top: 0;
        bottom: 0;
        right: 0;
        left: 0;
        content: "";
        position: absolute;
        background: rgba(0,0,0,.4);
        transition: all .15s ease-in;
      }
      .video-overlay {
        p {
          {% css_block theme.gallery_video_link_typography %};
          text-decoration-skip: ink;
          vertical-align: bottom;
          transition: all .15s ease-in;
        }
        transition: opacity .6s cubic-bezier(0.19, 1, 0.22, 1);
        .play-icon {
          color: {{ theme.gallery_video_link_typography.color }};
          font-size: {{ theme.gallery_video_link_typography.font-size | divided_by: 1.5 | append: 'px' }};
          transition: all .15s ease-in;
          margin-left: .3em;
          margin-top: .4em;
          text-decoration: none;
          vertical-align: bottom;
        }
        &:hover {
          &:before {
            background: rgba(0,0,0,.5);
          }
          p {
            {% css_block theme.gallery_video_link_typography.hover %};
          }
          .play-icon {
            transform: translateX(.15rem);
            color: {{ theme.gallery_video_link_typography.hover.color }};
          }
        }
      }
      &.playing {
        .video-overlay {
          opacity: 0;
          pointer-events: none;
        }
      }
    }
    &.image, &.video {
      {% if theme.gallery_frame_size == 'Small' %}
        padding: 5px;
        @media #{$breakpoint-large}{
          padding: 10px;
        }
      {% elsif theme.gallery_frame_size == 'Medium' %}
        padding: 1rem;
        @media #{$breakpoint-large}{
          padding: 3rem;
        }
      {% elsif theme.gallery_frame_size == 'Large' %}
        padding: 2rem;
        @media #{$breakpoint-large}{
          padding: 5rem;
        }
      {% else %}
        padding: 0;
      {% endif %}
      {% if theme.gallery_frames_color == false %}
        &:before {
          background: rgba(0,0,0,.2);
          content: "";
          position: absolute;
          height: 100%;
          width: 100%;
          left: 0;
          top: 0;
          bottom: 0;
          right: 0;
          z-index: 1;
        }
      {% endif %}
    }
    &.text + .text {
      div {
        margin-top: 0;
      }
    }
    &.text + .pagination > div{
      padding-top: 3rem;
    }
    &.text {
      background-color: {{ theme.background_color }}; 
      p:not(:last-child) {
        margin-bottom: 3rem;
      }
    }
  }
  {% if theme.gallery_layout == '1 then 2 Columns' %}
    .asset:not(.text):nth-child(3n + 1) {
      width: 100%;
      padding: 0;
      .caption {
        padding-left: 3rem;
        div {
          max-width: 40%;
        }
      }
      .portrait, .square {
        width: auto;
        // height: 100%;
        max-height: 100vh;
      }
      &.portrait, &.square {
        {% if theme.gallery_frame_size == 'Small' %}
          padding: 5px;
          @media #{$breakpoint-large}{
            padding: 10px;
          }
        {% elsif theme.gallery_frame_size == 'Medium' %}
          padding: 1rem;
          @media #{$breakpoint-large}{
            padding: 3rem;
          }
        {% elsif theme.gallery_frame_size == 'Large' %}
          padding: 2rem;
          @media #{$breakpoint-large}{
            padding: 5rem;
          }
        {% else %}
          padding: 0;
        {% endif %}
        .caption {
          padding-left: 0;
        }
      }
      &.video .video-overlay {
        p {
          font-size: {{ theme.gallery_video_link_typography.font-size | times: 1.8 | append: 'px' }};
        }
        .play-icon {
          font-size: {{ theme.gallery_video_link_typography.font-size | times: 1.3 | append: 'px' }};
        }
      }
    }
  {% endif %}
  .asset.image:nth-child(6n) .img-container {
    justify-content: flex-end;
  }
  .asset.image:nth-child(6n + 2) .img-container {
    justify-content: flex-start;
  }
  .asset.image:nth-child(5n + 1) {
    align-items: flex-end;
  }
  .asset.image:nth-child(7n + 1) {
    align-items: center;
  }
}

.asset.image img {
  transition: all .6s cubic-bezier(0.19, 1, 0.22, 1);
  &:hover {
    // opacity: .8 !important;
  }
}

.image-placeholder {
  max-width: 100%;
  height: auto;
  transition: opacity 500ms ease-in-out;
}

/*
------------------------------------------------------------------
Collection loop
------------------------------------------------------------------
*/

.homepage-container {
  .asset {
    img {
      transition: all 0.5s cubic-bezier(0.19, 1, 0.22, 1);
      opacity: 0;
      position: relative;
      object-fit: cover;
      left: 0;
      right: 0;
      bottom: 0;
      max-width: 100%;
      max-height: 100%;
      min-height: 100%;
      opacity: 0;
      &.lazyloaded {
        opacity: 1;
      }
      @media #{$breakpoint-large}{
        position: absolute;
      }
    }
    .overlay, .title {
      opacity: 0;
      transition: all 0.5s cubic-bezier(0.19, 1, 0.22, 1);
    }
    .overlay {
      padding: 2rem;
      @media #{$breakpoint-large}{
        padding: 5rem;
      }
    }
    .title {
      transform: translateX(-5%);
      transition: all .6s cubic-bezier(0.19, 1, 0.22, 1);
    }
    @media #{$breakpoint-large}{
      &:hover:not(.title-image) {
        img.lazyloaded {
          opacity: 0 !important;
        }
        .overlay, .title {
          opacity: 1;
        }
        .title {
          transform: translateX(0);
        }
      }
    }
    {% if theme.listings_layout != 'masonry' %}
      .img-container {
        {% if theme.listing_frame_size == 'Small' %}
          margin: 5px;
          @media #{$breakpoint-large}{
            margin: 10px;
          }
        {% elsif theme.listing_frame_size == 'Medium' %}
          margin: 1rem;
          @media #{$breakpoint-large}{
            margin: 3rem;
          }
        {% elsif theme.listing_frame_size == 'Large' %}
          margin: 2rem;
          @media #{$breakpoint-large}{
            margin: 5rem;
          }
        {% else %}
          margin: 0;
        {% endif %}
      }
    {% endif %}
    {% if theme.listings_layout == 'masonry' %}
      .img-container {
        {% if theme.listing_frame_size == 'Small' %}
          padding: 5px;
          @media #{$breakpoint-large}{
            padding: 10px;
          }
        {% elsif theme.listing_frame_size == 'Medium' %}
          padding: 1rem;
          @media #{$breakpoint-large}{
            padding: 2rem;
          }
        {% elsif theme.listing_frame_size == 'Large' %}
          padding: 2rem;
          @media #{$breakpoint-large}{
            padding: 5rem;
          }
        {% else %}
          padding: 0;
        {% endif %}
      }
    {% endif %}
    {% if theme.listing_frames_color %}
      a {
        background-color: {{ theme.listing_frames_color_code }} !important;
      }
      .overlay {
        background: none;
      }
    {% endif %}

    .title {
      p {
        {% css_block theme.listing_caption_typography %};
        text-decoration-skip: ink;
      }
      span {
        {% css_block theme.listing_caption_typography %};
        font-family: {{ theme.listing_caption_text_font.font-family }};
        font-weight: Normal;
        text-decoration: none;
        font-size: {{ theme.listing_caption_typography.font-size | divided_by: 2 | append:'px'  }};
      }
    }
  }
  .inner-asset-container {
    grid-column-gap:  {% if theme.listing_grid_size == 'Small' %}2px{% elsif theme.listing_grid_size == 'Medium' %}3rem{% elsif theme.listing_grid_size == 'Large' %}5rem{% else %}0{% endif %};
    grid-row-gap:  {% if theme.listing_grid_size == 'Small' %}2px{% elsif theme.listing_grid_size == 'Medium' %}3rem{% elsif theme.listing_grid_size == 'Large' %}5rem{% else %}0{% endif %};
    {% if theme.listing_grid_size == 'Small' %}
      margin-bottom: 2px;
    {% elsif theme.listing_grid_size == 'Medium' %}
      margin-bottom: 1rem;
      @media #{$breakpoint-large}{
        margin-bottom: 3rem;
      }
    {% elsif theme.listing_grid_size == 'Large' %}
      margin-bottom: 2rem;
      @media #{$breakpoint-large}{
        margin-bottom: 5rem;
      }
    {% else %}
      margin-bottom: 0;
    {% endif %}
    .asset {
      {% if theme.listing_grid_size == 'Small' %}
        margin-bottom: 2px;
        @media #{$breakpoint-large}{
          margin-bottom: 0;
        }
      {% elsif theme.listing_grid_size == 'Medium' %}
        margin-bottom: 1rem;
        @media #{$breakpoint-large}{
          margin-bottom: 0;
        }
      {% elsif theme.listing_grid_size == 'Large' %}
        margin-bottom: 2rem;
        @media #{$breakpoint-large}{
          margin-bottom: 0;
        }
      {% else %}
        margin-bottom: 0;
      {% endif %}
      img {
        &.portrait, .square {
          height: auto;
          width: 100%;
        }
      }
      .overlay {
        background: rgba(0,0,0,.4)
      }
    }
  }
  {% if theme.listings_layout == '1x2' %}
    .inner-asset-container {
      @media #{$breakpoint-large}{
          display: grid;
          grid-template-columns: repeat( 12, 1fr );
          grid-template-rows: repeat( 2, 1fr );
          max-height: 1000px;
          height: 90vh;
        .asset {
          &:first-child {
            grid-column-end: 9;
            grid-column-start: 1;
            grid-row-start: 1;
            grid-row-end: 3;
            .title p {
              font-size: {{ theme.listing_caption_typography.font-size | times: 1.5 | append:'px' }};
            }
          }
          &:nth-child(2), &:nth-child(3) {
            grid-column-end: 13;
            grid-column-start: 9;
          }
        }
        &:nth-child(even) {
          .asset {
            &:first-child {
              grid-column-end: 13;
              grid-column-start: 5;
            }
            &:nth-child(2), &:nth-child(3) {
              grid-column-end: 5;
              grid-column-start: 1;
            }
          }
        }
      }
    }
  {% elsif theme.listings_layout == '2x1' %}
    .inner-asset-container {
      @media #{$breakpoint-large}{
          height: 200vh;
          max-height: 2000px;
          display: grid;
          grid-template-columns: repeat( 12, 1fr );
          grid-template-rows: repeat( 4, 1fr );
        .asset {
          &:nth-child(2) {
            grid-column-end: 7;
            grid-column-start: 1;
            grid-row-start: 3;
            grid-row-end: 5;
          }
          &:nth-child(3) {
            grid-column-end: 13;
            grid-column-start: 7;
            grid-row-start: 3;
            grid-row-end: 5;
          }
          &:nth-child(1) {
            grid-column-end: 13;
            grid-column-start: 1;
            grid-row-start: 1;
            grid-row-end: 3;
            .title p {
              font-size: {{ theme.listing_caption_typography.font-size | times: 1.5 | append:'px' }};
            }
          }
        }
      }
    }
  {% elsif theme.listings_layout == 'masonry' %}
    .asset-container {
      @media #{$breakpoint-large}{
        column-count: 2;
        column-gap: {% if theme.listing_grid_size == 'Small' %}2px{% elsif theme.listing_grid_size == 'Medium' %}3rem{% elsif theme.listing_grid_size == 'Large' %}5rem{% else %}0{% endif %};
      }
      .asset {
        {% if theme.listing_grid_size == 'Small' %}
          margin-bottom: 2px;
        {% elsif theme.listing_grid_size == 'Medium' %}
          margin-bottom: 1rem;
          @media #{$breakpoint-large}{
            margin-bottom: 3rem;
          }
        {% elsif theme.listing_grid_size == 'Large' %}
          margin-bottom: 2rem;
          @media #{$breakpoint-large}{
            margin-bottom: 5rem;
          }
        {% else %}
          margin-bottom: 0;
        {% endif %}
        .img-container, a {
          display: block;
        }
        img {
          position: relative;
          left: inherit;
          right: inherit;
          min-width: none;
          min-height: none;
          width: 100%;
          height: auto;
        }
      }
    }
  {% endif %}
}

.asset.text, .title-element, .pagination {
  a {
    {% css_block theme.link_typography %};
    text-decoration-skip: ink;
    &:hover {
      {% css_block theme.link_typography.hover %};
    }
  }
}


